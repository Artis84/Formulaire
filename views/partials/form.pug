<form action="/form" method="post">
    <div class="form-group">
        <label for="inputkey" class="form-label mt-4">Clé</label>
        <input type="text" class="form-control" id="inputkey" name="key" aria-describedby="Boite de saisie de la clé" placeholder="clé" value="CLE-TEST-IOT">
        <label for="inputEmail" class="form-label mt-4">Adress e-mail</label>
        <input type="email" class="form-control" id="inputEmail" name="email" aria-describedby="Boite de saisie de l'email" placeholder="Votre email">
        <label for="inputDate" class="form-label mt-4">Date</label>
        <input type="date" class="form-control" id="inputDate" name="date" aria-describedby="Boite de saisie de la date" placeholder="JJ/MM/AAAA">
        <label for="inputUrl" class="form-label mt-4">GitHub Url</label>
        <input type="url" class="form-control" id="inputUrl" name="url" aria-describedby="Boite de saisie pour l'url du répertoire gitHub" placeholder="https://github.com/nom/votre-repertoire">
        <label for="inputMessage" class="form-label mt-4">🚨❗ Message top secret 🚨❗</label>
        <textarea class="form-control" id="inputMessage" name="message" rows="3" placeholder="Votre message"></textarea>
    </div>
    <button type="submit" class="btn btn-primary">Envoyer 🐤</button>
</form>

